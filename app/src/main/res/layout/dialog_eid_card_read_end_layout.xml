<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/eid_px_335"
    android:layout_height="wrap_content"
    android:layout_gravity="center_horizontal"
    android:background="@color/froad_white"
    android:orientation="vertical"
    >

    <TextView
        android:id="@+id/sure_tv"
        style="@style/common_tv_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/eid_px_25"
        android:layout_gravity="center"
        android:text="身份信息读取完成"
        />

    <View
        android:id="@+id/view_line_1"
        style="@style/view_line"
        android:layout_marginTop="@dimen/eid_px_25"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/sure_btn"
            style="@style/common_tv_large_style"
            android:layout_width="match_parent"
            android:layout_height="@dimen/eid_px_46"
            android:gravity="center"
            android:text="确定"
            android:textColor="#FFEF571F"
            />
    </LinearLayout>
</LinearLayout>
